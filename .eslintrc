{
    "plugins": ["html", "jinja2"],
    "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
    "env": {
        "browser": true,
        "commonjs": true,
        "jquery": true,
        "es6": true
    },
    "ignorePatterns": ["node_nodules"],
    "rules": {
        "jsx-a11y/label-has-associated-control": "off",
        "import/no-cycle": "off"
    }
}
