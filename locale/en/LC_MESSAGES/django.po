#: protokolle/templates/protokolle/delete_attachment.html:14
#, python-format
msgid ""
"Soll der Anhang <a href=\"%(url)s\">%(name)s</a> wirklich gelöscht werden?"
msgstr ""
"Do you really want to delete the attachment <a href=\"%(url)s\">%(name)s</a>?"

#: protokolle/templates/protokolle/delpad.html:8
#, python-format
msgid "Pad für %(title)s am %(time)s löschen"
msgstr "Delete pad for %(title)s at %(time)s"

#: protokolle/templates/protokolle/delpad.html:16
#, python-format
msgid ""
"Soll das Pad für <a href=\"%(url)s\">%(name)s am %(time)s</a> (Stand: "
"%(last_edit_pad)s) wirklich gelöscht werden?"
msgstr ""
"Do you really want to delete the pad for <a href=\"%(url)s\">%(name)s at "
"%(time)s</a> (as of %(last_edit_pad)s)?"

#: protokolle/templates/protokolle/delpad.html:30
